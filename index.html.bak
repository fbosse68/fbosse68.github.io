        /* Navigation Extensions für index */
        nav {
            border-bottom: 1px solid rgba(91, 123, 143, 0.08);
            transition: transform 0.4s cubic-bezier(0.4, 0, 0.2, 1);
        }

        nav.hide-on-scroll {
            transform: translateY(-100%);
        }

        @keyframes slideDown {
            from {
                transform: translateY(-100%);
                opacity: 0;
            }
            to {
                transform: translateY(0);
                opacity: 1;
            }
        }

        .nav-links {
            display: flex;
            gap: 3rem;
            list-style: none;
        }

        .nav-links a {
